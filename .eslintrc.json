{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier", "@typescript-eslint"],
    "rules": {
      "camelcase": "off",
      "@typescript-eslint/name-convention":[
        "error",
        {
          "selector": "interface",
          "format": ["PascalCase"],
          "custom":{
            "regex":"^I[A-Z]",
            "match" : "true"
          } 
        }
      ]
    }
}
